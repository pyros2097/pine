package main

import routes
import html

fun calculateDiscount(price: int, quantity: int, percentage: float) -> int
    t := time.Now()
    println(t)
    t2 := t.addDate(-3, 0, 0)
    println(t2)
    return (price * quantity * 100) / percentage
end

@route("/about/{name}")
fun about(name: string) -> Html
    return (
        <Window x="1" y="2" width="480" height="800">
            <VBox>
                <Text>This is my ${name}<Text>
            </VBox>
        </Window>
    )
end
