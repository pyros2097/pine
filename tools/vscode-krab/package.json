{
  "name": "krab",
  "displayName": "krab",
  "description": "Krab language",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.35.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:krab"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "krab",
        "aliases": [
          "Krab",
          "krab"
        ],
        "extensions": [
          ".kr"
        ]
      },
      {
        "id": "krab",
        "extensions": [
          ".krab"
        ],
        "aliases": [
          "Krab"
        ]
      },
      {
        "id": "krab.mod",
        "extensions": [
          "krab.mod"
        ],
        "aliases": [
          "Krab Module File"
        ]
      },
      {
        "id": "krab.sum",
        "extensions": [
          "krab.sum"
        ],
        "aliases": [
          "Krab Checksum File"
        ]
      }
    ],
    "grammars": [
      {
        "language": "krab",
        "scopeName": "source.krab",
        "path": "./syntaxes/krab.json"
      },
      {
        "language": "krab.mod",
        "scopeName": "krab.mod",
        "path": "./syntaxes/krab.mod.json"
      },
      {
        "language": "krab.sum",
        "scopeName": "krab.sum",
        "path": "./syntaxes/krab.sum.json"
      }
    ],
    "commands": [
      {
        "command": "krab.run.file",
        "title": "Run selected Krab file",
        "category": "Krab"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "eslint": "^5.13.0",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
